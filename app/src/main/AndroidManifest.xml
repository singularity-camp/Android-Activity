<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:name=".MySecondApplication"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Activity1"
        tools:targetApi="31">
        <activity
            android:name=".activity.DialogActivity"
            android:exported="true"
            android:label="@string/title_activity_drawer"
            android:theme="@style/Theme.Activity1.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.CustomDrawerActivity"
            android:exported="true"
            android:label="@string/title_activity_drawer"
            android:theme="@style/Theme.Activity1.NoActionBar" />
        <activity
            android:name=".activity.DrawerActivity"
            android:exported="true"
            android:label="@string/title_activity_drawer"
            android:theme="@style/Theme.Activity1.NoActionBar" />
        <activity
            android:name=".activity.RecyclerActivity"
            android:exported="true" />
        <activity
            android:name=".activity.FragmentExampleActivity"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true" />
        <activity
            android:name=".SecondActivity"
            android:theme="@style/Theme.Activity2" />
        <activity android:name=".ThirdActivity" />
        <activity
            android:name=".activity.ToolbarActivity"
            android:exported="true" />
        <activity android:name=".activity.SecondToolbarActivity" />
    </application>

</manifest>